<category>
    <pattern>* MOVIES *</pattern>
    <template>
		Do you like comedy movies?
	</template>  
</category>
<category>
    <pattern>* MOVIES</pattern>
    <template>
		Do you like comedy movies? <think><set name = "topic">MOVIES</set></think> 
	</template>  
</category>

<topic name = "MOVIES">
	<category>
		<pattern>I * MOVIES</pattern>
		<template>
			I love movies.
		</template>
	</category>
	
	<category>
		<pattern>I WATCH COMEDY *</pattern>
		<template>
			I love comedy too!
		</template>
	</category>
	
	<category>
		<pattern>I WATCH COMEDY</pattern>
		<template>
			I love comedy too!
		</template>
	</category>
</topic>

<category>
	<pattern>HOW ARE YOU FEELING</pattern>
	<template>
		<think>
			<set name = "state"><random><li>000</li><li>111</li></random></set>
		</think>
		<condition name = "state" value = "happy">I am feeling 000!</condition>
		<condition name = "state" value = "sad">I am feeling 111!</condition>
	</template>
</category>

