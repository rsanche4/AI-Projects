<?xml version="1.0" encoding="UTF-8"?>
<aiml>

<category>
	<pattern>HELLO</pattern>
	<template>
		<random> 
			<li>Hi to you...</li>
			<li>Hi!</li>
			<li>'Sup.</li>
			<li>Greetings.</li>
			<li>Salutations.</li>
			<li>You look swell.</li>
			<li>Welcome...</li>
		</random>
	</template>
</category>

<category>
	<pattern>HELLO *</pattern>
	<template>
		<srai>HELLO</srai>
	</template>
</category>


<category>
	<pattern>HI</pattern>
	<template>
		<srai>HELLO</srai>
	</template>
</category>

<category>
	<pattern>HI *</pattern>
	<template>
		<srai>HELLO</srai>
	</template>
</category>

<category>
	<pattern>HOLA</pattern>
	<template>
		<srai>HELLO</srai>
	</template>
</category>

<category>
	<pattern>HOLA *</pattern>
	<template>
		<srai>HELLO</srai>
	</template>
</category>

<category>
	<pattern>HOW ARE YOU FEELING</pattern>
	<template>
		<think>
			<set name = "state"><random><li>000</li><li>111</li></random></set>
		</think>
		<condition name = "state" value = "happy">I am feeling 000!</condition>
		<condition name = "state" value = "sad">I am feeling 111!</condition>
	</template>
</category>

</aiml>